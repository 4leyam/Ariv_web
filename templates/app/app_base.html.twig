{% block customstyle %}
    <style>

        body {
            font-size: 15px;
        }
        .mastfoot {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            background-color: #F6F7EB;
            text-align: center;
            padding: 0px;
        }
    </style>
{% endblock %}

{% block header %}

    {{  include('app/app_navbar.html.twig' , { 'page' : page }) }}

{% endblock %}

{% block footer %}
    <!-- mise en place du footer de l'app -->
